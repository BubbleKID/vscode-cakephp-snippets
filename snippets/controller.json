{
  "var_dump": {
    "prefix": "vd",
    "body": ["var_dump($1);","$0"],
    "description": "PHP: var_dump()"
  },
  "debug": {
    "prefix": "db",
    "body": ["debug($1);","$0"],
    "description": "CakePHP: debug()"
  },
  "die(json_encode())": {
    "prefix": "dj",
    "body": ["die(json_encode($1));","$0"],
    "description": "CakePHP: die(json_encode())"
  },
  "$this->loadModel()": {
    "prefix": "lm",
    "body": ["\\$this->loadModel('$1');","$0"],
    "description": "CakePHP: $this->loadModel()"
  },
  "find": {
    "prefix": "find",
    "body": ["\\$this->${1:Model}->find('${2:all}'$0);"],
    "description": "CakePHP: $this->find()"
  },
  "data": {
    "prefix": "data",
    "body": ["\\$this->request->data$0"],
    "description": "CakePHP: $this->request->data"
  },
  "save": {
    "prefix": "save",
    "body": ["\\$this->${1:Model}->save(${2:\\$this->request->data})${3:;}$0"],
    "description": "CakePHP: $this->Model->save()"
  },
  "pr": {
    "prefix": "pr",
    "body": ["pr($0);exit;"],
    "description": "CakePHP: pr();exit;"
  },
  "set": {
    "prefix": "set",
    "body": ["\\$this->set($0);"],
    "description": "CakePHP: $this->set()"
  },
  "allow ": {
    "prefix": "allow",
    "body": ["\\$this->Auth->allow(${1:array($2)})"],
    "description": "CakePHP: $this->->Auth->allow()"
  },
  "authenticate ": {
    "prefix": "authenticate",
    "body": ["\\$this->Auth->authenticate = array($1);"],
    "description": "CakePHP: $this->->Auth->authenticate()"
  },
  "deny": {
    "prefix": "deny",
    "body": ["\\$this->Auth->deny(${1:array($2)})"],
    "description": "CakePHP: $this->->Auth->deny()"
  },
  "loggedIn": {
    "prefix": "loggedIn",
    "body": ["\\$this->Auth->loggedIn()"],
    "description": "CakePHP: $this->->Auth->loggedIn()"
  },
  "login": {
    "prefix": "login",
    "body": ["\\$this->Auth->login($1)"],
    "description": "CakePHP: $this->->Auth->login()"
  },
  "logout": {
    "prefix": "logout",
    "body": ["\\$this->Auth->logout()"],
    "description": "CakePHP: $this->->Auth->logout()"
  },
  "mapActions": {
    "prefix": "mapActions",
    "body": ["\\$this->Auth->mapActions(${1:array($2)})"],
    "description": "CakePHP: $this->->Auth->mapActions()"
  },
  "redirectUrl": {
    "prefix": "redirectUrl",
    "body": ["\\$this->Auth->redirectUrl()"],
    "description": "CakePHP: $this->->Auth->redirectUrl()"
  },
  "flash": {
    "prefix": "flash",
    "body": ["\\$this->Session->flash();$0"],
    "description": "CakePHP: $this->Session->flash()"
  },
  "check": {
    "prefix": "check",
    "body": ["\\$this->Session->check(${1:\\$name});$0"],
    "description": "CakePHP: $this->Session->check()"
  },
  "delete": {
    "prefix": "delete",
    "body": ["\\$this->Session->delete(${1:\\$name});$0"],
    "description": "CakePHP: $this->Session->delete()"
  },
  "destroy": {
    "prefix": "destroy",
    "body": ["\\$this->Session->destroy();$0"],
    "description": "CakePHP: $this->Session->destroy()"
  },
  "read": {
    "prefix": "read",
    "body": ["\\$this->Session->read(${1:\\$name});$0"],
    "description": "CakePHP: $this->Session->read()"
  },
  "setFlash": {
    "prefix": "setFlash",
    "body": ["\\$this->Session->setFlash(${1:\\$message}, ${2:\\$element = 'default'}, ${3:\\$params = array()}, ${4:\\$key = 'flash'});$0"],
    "description": "CakePHP: $this->Session->setFlash()"
  },
  "write": {
    "prefix": "write",
    "body": ["\\$this->Session->write(${1:\\$name},${2:\\$value});"],
    "description": "CakePHP: $this->Session->write()"
  },
  "conditions": {
    "prefix": "conditions",
    "body": ["'conditions' => array('${1:Model}.${2:id}' => ${3:\\$id})$0"],
    "description": "CakePHP: 'conditions' => array()"    
  },
  "order": {
    "prefix": "order",
    "body": ["'order' => array('${1:Model}.${2:id}' => '${3:ASC}')$0"],
    "description": "CakePHP: 'order' => array()"   
  },
  "contain": {
    "prefix": "contain",
    "body": ["'contain' => array('${1:Model}')$0"],
    "description": "CakePHP: 'contain' => array()"  
  },
  "url-array": {
    "prefix": "url",
    "body": ["array('controller' => '$1', 'action' => '$2'${3:, });$0"],
    "description": "CakePHP: array('controller'=>'','action'=>'')"
  },
  "$this->request": {
    "prefix": "request",
    "body": ["\\$this->request$0"],
    "description": "CakePHP: '$this->request"
  },
  "$this->redirect": {
    "prefix": "redirect",
    "body": ["\\$this->redirect($0);"],
    "description": "CakePHP: '$this->redirect()"
  },
  "$this->request->params": {
    "prefix": "params",
    "body": ["\\$this->request->params$0"],
    "description": "CakePHP: '$this->request->params"
  },
  "$this->request->query": {
    "prefix": "query",
    "body": ["\\$this->request->query$0"],
    "description": "CakePHP: '$this->request->query"
  }
}